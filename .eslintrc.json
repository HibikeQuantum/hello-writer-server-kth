{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {},
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "import", "prettier"
  ],
  "rules": {
    "camelcase": "off",
    "no-console": "off",
    "indent": "off",
    "object-curly-spacing": "off",
    "import/no-unresolved": "off",
    "import/extensions":"off",
    "no-plusplus": "off"
  }
}
